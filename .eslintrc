{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "plugins": ["react", "eslint-plugin-react"],
  "rules": {
    "no-unused-vars": 1,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "no-undef": "warn"
  }
}
